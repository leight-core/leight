	"description": "A component library for React and Ant Design.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/leight-core/leight.git"
	},
	"source": "src/index.js",
	"module": "dist/leight.modern.js",
	"types": "flow-typed",
	"files": [
		"dist",
		"flow-typed"
	],
	"scripts": {
		"flow": "flow",
		"flow-typed": "flow-remove-types src -d flow-typed",
		"start": "microbundle watch",
